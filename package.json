{
  "name": "ecma6-web-app",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "doctor": "./node_modules/.bin/clinic doctor -- node src/index.js",
    "flame": "./node_modules/.bin/clinic flame -- node src/index.js",
    "bubbleprof": "./node_modules/.bin/clinic bubbleprof -- node src/index.js",
    "test": "HTTP_PORT=0 ava --verbose",
    "snyk-protect": "./node_modules/.bin/snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "author": "",
  "license": "ISC",
  "ava": {
    "files": [
      "test/appTest.js",
      "test/**/*",
      "!test/config",
      "!test/mocks"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "require": [
      "./test/config/index.js"
    ],
    "verbose": true,
    "nodeArguments": [
      "--trace-deprecation",
      "--napi-modules"
    ]
  },
  "devDependencies": {
    "ava": "3.5.1",
    "ava-http": "^1.0.0",
    "clinic": "5.0.1",
    "danger": "10.1.1",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "lerna": "^3.22.1",
    "mongodb-memory-server": "^6.6.1",
    "test-listen": "^1.1.0",
    "testdouble": "3.13.1"
  },
  "dependencies": {
    "@sendgrid/client": "6.5.5",
    "airtable": "0.10.1",
    "awilix": "4.2.6",
    "aws-sdk": "2.814.0",
    "bunyan": "1.8.13",
    "connect-mongo": "3.2.0",
    "dotenv": "8.2.0",
    "esm": "3.2.25",
    "express-session": "1.17.1",
    "mongoose": "5.11.7",
    "opentok": "2.10.0",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "pubnub": "4.27.5",
    "restify": "8.5.1",
    "rollbar": "2.15.1",
    "snyk": "1.463.0",
    "socket.io": "3.0.0",
    "twilio": "4.7.0"
  },
  "snyk": true
}
